{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tchap_no_updater",
  "version": "4.18.3",
  "identifier": "fr.gouv.beta.tchap-desktop",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "windows": {
      "allowDowngrades": true,
      "nsis": {
        "installMode": "currentUser"
      },
      "wix": {
        "language": "fr-FR"
      }
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": false,
    "resources": {
      "sqlcipher.dll": "",
      "libcrypto-3-x64.dll": "",
      "libssl-3-x64.dll": "",
      "resources/windows/msvcp140.dll": "./msvcp140.dll",
      "resources/windows/vcruntime140.dll": "./vcruntime140.dll"
    }
  },
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": [
          "tchap"
        ]
      }
    },
    "protocol": {
      "schemas": [
        "tchap"
      ]
    }
  }
}
